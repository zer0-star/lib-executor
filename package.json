{
  "name": "lib-executor",
  "displayName": "lib-executor",
  "description": "expand your lib",
  "version": "0.1.1",
  "publisher": "zer0-star",
  "repository": {
    "url": "https://github.com/zer0-star/lib-executor.git"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lib-executor.execute"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "lib-executor",
      "properties": {
        "lib-executor.libraryPath": {
          "title": "Library Path",
          "type": "string",
          "default": "/path/to/your_library",
          "description": "Path to your library"
        }
      }
    },
    "commands": [
      {
        "command": "lib-executor.execute",
        "title": "lib-executor: execute"
      }
    ],
    "keybindings": [
      {
        "mac": "cmd+ctrl+e",
        "command": "lib-executor.execute"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
